<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h3>Add Premium Collaborators</h3>
    <%= form_for [@wiki, @collaborator] do |f| %>
    <div class="form-group">
      <%= f.label :Premium %>
      <%= f.select :user_id, options_from_collection_for_select(User.premium, "id", "email") %>
    </div>

    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
    </div>
    <% end %>
  </div>
</div>

